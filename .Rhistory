log_summed_exps <- function(x){
max_x <- max(x)
min_x <- min(x)
sum_exp <- sum(exp(sort(x[-1])-max_x))
log_sum <- 1 + log(sum_exp)
output_sum <- min_x + log_sum
return output_sum
}
